{
  "name": "@consensus-shipyard/ipc-linked-token",
  "version": "1.0.0-alpha.0",
  "description": "A set of contracts to replicate a pre-existing ERC20 token from one network (parent) to another (child).",
  "author": "ConsensusLab, Protocol Labs",
  "packageManager": "pnpm@9.3.0",
  "homepage": "https://github.com/consensus-shipyard/ipc/",
  "license": "MIT OR Apache-2.0",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "prepack": "sh -c 'cp ../../LICENSE-* .'",
    "postpack": "sh -c 'rm -f LICENSE-*'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/consensus-shipyard/ipc.git"
  },
  "publishConfig": {
